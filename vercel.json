{
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Method",
          "value": "GET,POST,PUT,DELETE"
        }
      ]
    }
  ],
  "rewrites": [
    {"source": "/get-comments(\/?)?:path*", "destination": "/api/get-comments/?:path*"},
    {"source": "/post-comment(\/?):path*", "destination": "/api/post-comment/"}
  ]
}