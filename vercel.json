{
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=99"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Method",
          "value": "GET,POST,PUT,DELETE"
        }
      ]
    }
  ],
  "rewrites": [
    {"source": "/(.*)", "destination": "/api/index/"}
  ],
  "redirects": [
    {"source": "/private", "destination": "/404.json", "permanent": true},
    {"source": "/api", "destination": "/404.json", "permanent": true},
    {"source": "/LICENSE", "destination": "/404.json", "permanent": true},
    {"source": "/README.md", "destination": "/404.json", "permanent": true}
  ]
}