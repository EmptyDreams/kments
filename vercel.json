{
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Method",
          "value": "GET,POST,PUT,DELETE"
        }
      ]
    }
  ],
  "rewrites": [
    {"source": "/get-comments(\/?)?:path*", "destination": "/api/get-comments/?:path*"},
    {"source": "/post-comment/", "destination": "/api/post-comment/"},
    {"source": "/get-recent-comments/", "destination": "/api/get-recent-comments/"},
    {"source": "/admin-certificate/", "destination": "/api/admin-certificate/"},
    {"source": "/auth-certificate/", "destination": "/api/auth-certificate/"},
    {"source": "/log-out/", "destination": "/api/log-out/"},
    {"source": "/delete-comments/", "destination": "/api/delete-comments/"},
    {"source": "/hide-comments/", "destination": "/api/hide-comments/"},
    {"source": "/update-comment/", "destination": "/api/update-comment/"},
    {"source": "/count-visit/", "destination": "/api/count-visit"},
    {"source": "/get-pages-view/", "destination": "/api/get-pages-view/"}
  ],
  "redirects": [
    {"source": "/(b|e|f|i|j|k|m|n|o|q|r|s|t|v|w|x|y|z)", "destination": "/404.json", "statusCode": 404},
    {"source": "/a([b-ce-tv-z])", "destination": "/404.json", "statusCode": 404},
    {"source": "/c([a-np-q])", "destination": "/404.json", "statusCode": 404},
    {"source": "/d([a-df-z])", "destination": "/404.json", "statusCode": 404},
    {"source": "/g([a-df-z])", "destination": "/404.json", "statusCode": 404},
    {"source": "/h([a-hj-z])", "destination": "/404.json", "statusCode": 404},
    {"source": "/l([a-np-z])", "destination": "/404.json", "statusCode": 404},
    {"source": "/p([a-np-z])", "destination": "/404.json", "statusCode": 404},
    {"source": "/u([a-oq-z])", "destination": "/404.json", "statusCode": 404}
  ]
}