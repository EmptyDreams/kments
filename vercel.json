{
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Method",
          "value": "GET,POST,PUT,DELETE"
        }
      ]
    }
  ],
  "rewrites": [
    {"source": "/get-comments(\/?)?:path*", "destination": "/api/get-comments/?:path*"},
    {"source": "/post-comment/", "destination": "/api/post-comment/"},
    {"source": "/get-recent-comments/", "destination": "/api/get-recent-comments/"},
    {"source": "/admin-certificate/", "destination": "/api/admin-certificate/"},
    {"source": "/auth-certificate/", "destination": "/api/auth-certificate/"},
    {"source": "/log-out/", "destination": "/api/log-out/"},
    {"source": "/delete-comments/", "destination": "/api/delete-comments/"},
    {"source": "/hide-comments/", "destination": "/api/hide-comments/"},
    {"source": "/update-comment/", "destination": "/api/update-comment/"},
    {"source": "(\/$)", "destination": "/api/index/"}
  ],
  "redirects": [
    {"source": "/private/:path*", "destination": "/api/private/", "permanent": true},
    {"source": "/api/lib/(.*)", "destination": "/api/private/", "permanent": true},
    {"source": "/favicon.(.*)", "destination": "/api/private/", "permanent": true}
  ]
}