{
  "rewrites": [
    {"source": "/(.*)", "destination": "/api/index"}
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Method",
          "value": "GET,POST,PUT,DELETE"
        }
      ]
    }
  ]
}